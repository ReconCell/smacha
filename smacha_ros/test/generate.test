<launch>
  <node pkg="smacha_ros" type="smacha_server" name="smacha_ros_generate_test" args="-v -t $(find smacha_ros)/src/smacha_ros/templates $(find smacha_ros)/test/smacha_templates/smacha_test_examples -s $(find smacha_ros)/test/smacha_scripts/smacha_test_examples" output="screen" />
  <test test-name="api_generate" pkg="smacha_ros" time-limit="120.0" type="api_generate_test.py" />
  <test test-name="service_generate" pkg="smacha_ros" time-limit="120.0" type="service_generate_test.py" />
</launch>
