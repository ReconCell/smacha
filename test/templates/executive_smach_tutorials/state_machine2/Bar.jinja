{% block base_header %}
# define state Bar
class Bar(smach.State):
    def __init__(self):
        smach.State.__init__(self, outcomes=['outcome1'])

    def execute(self, userdata):
        rospy.loginfo('Executing state {{ name }}')
        return 'outcome1'
{% endblock base_header %}

{% block body %}
smach.{{ parent_type }}.add('{{ name }}', Bar(){% if transitions is defined %}, 
                       transitions={{ '{' }}{% for outcome, transition in transitions.iteritems() | sort %}'{{ outcome }}':'{{ transition }}'{% if not loop.last %}, {% endif %}{% endfor %}{{ '}' }}{% endif %})
{% endblock body %}

{% block base_footer %}
{% endblock base_footer %}
